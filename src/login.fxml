<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <Label layoutX="48.0" layoutY="62.0" text="RestaurantName" />
      <Label layoutX="48.0" layoutY="124.0" text="Password" />
      <TextField fx:id="resNameUserText" layoutX="169.0" layoutY="58.0" />
      <PasswordField fx:id="passwordText" layoutX="169.0" layoutY="120.0" />
      <ButtonBar layoutX="124.0" layoutY="190.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetAction" text="Reset" />
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginAction" text="Login" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
